            <th style="width:10%;text-align:center">(24)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var row in (List<Web.Portal.Common.ViewModel.InventoryCustomViewModel>)ViewData["ListInventory"])
                {
                    PageCurrent += 1;

                    <tr>
                        <td>@PageCurrent</td>
                        <td>@Html.Raw(row.Cont)</td>
                        <td>@Html.Raw(row.SealNo)</td>
                        <td>@Html.Raw(row.GoodsName)</td>
                        <td>@Html.Raw(row.Pieces_weight)</td>
                        <td>@Html.Raw(row.Shipper)</td>
                        <td>@Html.Raw(row.Consignee)</td>
                        <td>@Html.Raw(row.AwbNo)</td>
                        <td>@Html.Raw(row.AwbDate)</td>
                        <td>@Html.Raw(row.AwbDate)</td>
                        <td>@Html.Raw(row.InventoryDate)</td>
                        <td>@Html.Raw(row.Location)</td>
                        <td>@Html.Raw(row.CustomManagement)</td>
                        <td>@Html.Raw(row.AwbStatus)</td>
                        <td>@Html.Raw(row.AwbType)</td>
                        <td>@Html.Raw(row.AwbOrigin)</td>
                        <td>@Html.Raw(row.ScanDate)</td>
                        <td>@Html.Raw(row.ScanQuestion)</td>
                        <td>@Html.Raw(row.CheckDate)</td>
                        <td>@Html.Raw(row.CheckResult)</td>
                        <td>@Html.Raw(row.Suggestion)</td>
                        <td>@Html.Raw(row.Cause)</td>
                        <td>@Html.Raw(row.Measure)</td>
                        <td>@Html.Raw(row.ProcessResult)</td>
                    </tr>
                }

            </tbody>

</table>
